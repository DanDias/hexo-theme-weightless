<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">

	<%
		var title = page.title;
	%>

	<title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
	<meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
	<% if (config.lunr) { %>
		<script src="<%= url_for('js/compatibility/augment.js') %>" type="text/javascript"></script>
	<% } %>
	<% if (config.feed) { %>
		<% if (config.feed.type == 'atom') { %>
			<link rel="alternate" href="<%= url_for(config.feed.path) %>" title="<%= config.title %>" type="application/atom+xml" />
		<% }else if (config.feed.type == 'rss2') { %>
			<link rel="alternate" href="<%= url_for(config.feed.path) %>" title="<%= config.title %>" type="application/rss+xml" />
		<% } %>
	<% } %>
	<link rel="stylesheet" href="<%= url_for('css/normalize.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/basscss.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/basscss-ui-groups.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/tomorrow.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/style.css') %>" type="text/css">
	<!-- ****** faviconit.com favicons ****** -->
	<link rel="shortcut icon" href="<%= url_for('favicons/favicon.ico') %>">
	<link rel="icon" sizes="16x16 32x32 64x64" href="<%= url_for('images/favicon.ico') %>">
	<link rel="icon" type="image/png" sizes="196x196" href="<%= url_for('favicons/favicon-192.png') %>">
	<link rel="icon" type="image/png" sizes="160x160" href="<%= url_for('favicons/favicon-160.png') %>">
	<link rel="icon" type="image/png" sizes="96x96" href="<%= url_for('favicons/favicon-96.png') %>">
	<link rel="icon" type="image/png" sizes="64x64" href="<%= url_for('favicons/favicon-64.png') %>">
	<link rel="icon" type="image/png" sizes="32x32" href="<%= url_for('favicons/favicon-32.png') %>">
	<link rel="icon" type="image/png" sizes="16x16" href="<%= url_for('favicons/favicon-16.png') %>">
	<link rel="apple-touch-icon" href="<%= url_for('favicons/favicon-57.png') %>">
	<link rel="apple-touch-icon" sizes="114x114" href="<%= url_for('favicons/favicon-114.png') %>">
	<link rel="apple-touch-icon" sizes="72x72" href="<%= url_for('favicons/favicon-72.png') %>">
	<link rel="apple-touch-icon" sizes="144x144" href="<%= url_for('favicons/favicon-144.png') %>">
	<link rel="apple-touch-icon" sizes="60x60" href="<%= url_for('favicons/favicon-60.png') %>">
	<link rel="apple-touch-icon" sizes="120x120" href="<%= url_for('favicons/favicon-120.png') %>">
	<link rel="apple-touch-icon" sizes="76x76" href="<%= url_for('favicons/favicon-76.png') %>">
	<link rel="apple-touch-icon" sizes="152x152" href="<%= url_for('favicons/favicon-152.png') %>">
	<link rel="apple-touch-icon" sizes="180x180" href="<%= url_for('favicons/favicon-180.png') %>">
	<meta name="msapplication-TileColor" content="#FFFFFF">
	<meta name="msapplication-TileImage" content="<%= url_for('favicons/favicon-144.png') %>">
	<meta name="msapplication-config" content="/browserconfig.xml">
	<!-- ****** faviconit.com favicons ****** -->
	<% if (theme.useGoogle) { %>
		<link href="http://fonts.googleapis.com/css?family=Lato:300,400,700,400italic&subset=latin,latin-ext" rel="stylesheet" type="text/css">
	<% }else{ %>
		<link href="https://dist.s3.fmt01.sdapi.net/fonts/lato-v11/lato-v11-all.css" rel="stylesheet" type="text/css">
	<% } %>
	<% if (config.google_site_verification) { %>
		<meta name="google-site-verification" content="<%= config.google_site_verification %>" />
	<% } %>

	<!--[if lt IE 9]><script src="<%- config.root %>js/html5.js" type="text/javascript"></script><![endif]-->
	<script src="<%= url_for('js/main.js') %>" type="text/javascript"></script>
</head>
